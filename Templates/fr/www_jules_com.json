  "TextTemplate": {
    "XPath": "/html/head/title"
  },
  "IconUrlTemplate": {
    "XPath": "/html/head/link[@rel=\"shortcut icon\"]/@href"
  },
  "Sections": [
    {
      "SectionName": "Liste des articles",
      "XPathFilter": "//ul[@id=\"search-result-items\"]",
      "Articles": [
        {
          "XPathFilter": "./li",
          "TitleTemplate": {
            "XPath": ".//a[@class=\"name-link\"]"
          },
          "TextTemplate": {
            "XPath": ".//span[@class=\"actualPrice\"]",
            "Regex": ""
          },
          "IconUrlTemplate": {
            "XPath": ".//div[@class=\"product-visuals\"]//img/@src"
          },
          "BrowserUrlTemplate": {
            "XPath": ".//a[@class=\"name-link\"]/@href",
            "Regex": ""
          }
        }
      ]
    }
  ]
}