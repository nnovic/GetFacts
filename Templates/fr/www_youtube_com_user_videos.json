{
  "PageName": "Youtube: vidéos de l'utilisateur",
  "Reference": "https://www.youtube.com/user/joueurdugrenier/videos",
  "TitleTemplate": {
    "XPath": "/html/head/title"
  },
  "TextTemplate": {
    "XPath": "/html/head/meta[@name=\"description\"]/@content"
  },
  "IconUrlTemplate": {
    "XPath": "//div[@id=\"appbar-nav\"]/a/img[@class=\"appbar-nav-avatar\"]/@src"
  },
  "Sections": [
    {
      "SectionName": "Vidéos",
      "XPathFilter": "//ul[@id=\"channels-browse-content-grid\"]",
      "Articles": [
        {
          "XPathFilter": "./li",
          "TitleTemplate": {
            "XPath": ".//div[@class=\"yt-lockup-content\"]/h3/a"
          },
          "TextTemplate": {
            "XPath": ".//div[@class=\"yt-lockup-content\"]"
          },
          "IconUrlTemplate": {
            "XPath": ".//img/@src"
          },
          "BrowserUrlTemplate": {
            "XPath": ".//div[@class=\"yt-lockup-content\"]/h3/a/@href"
          }
        }
      ]
    }
  ]
}