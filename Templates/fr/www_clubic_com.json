{
  "PageName": "Accueil Clubic",
  "Reference": "http://www.clubic.com/",
  "TitleTemplate": {
    "XPath": "/html/head/title"
  },
  "TextTemplate": {
    "XPath": "/html/head/meta[@name=\"description\"]/@content"
  },
  "IconUrlTemplate": {
    "XPath": "/html/head/meta[@property=\"og:image\"]/@content"
  },
  "Sections": [
    {
      "SectionName": "A la une",
      "XPathFilter": "//div[@data-vr-zone=\"Zone Meta\"]",
      "Articles": [
        {
          "XPathFilter": "./div",
          "TitleTemplate": {
            "XPath": ".//a[@class=\"panel-title\"]/text()"
          },
          "IconUrlTemplate": {
            "XPath": ".//div/@data-src"
          },
          "BrowserUrlTemplate": {
            "XPath": ".//a[@class=\"panel-title\"]/@href"
          }
        }
      ]
    }
  ]
}